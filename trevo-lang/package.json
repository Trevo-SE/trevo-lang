{
    "name": "trevo-lang",
    "displayName": "trevo_lang",
    "description": "Trevo Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "trevo-lang",
            "aliases": ["trevo", "trevo-lang"],
            "extensions": [".tr"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "trevo-lang",
            "scopeName": "source.tr",
            "path": "./syntaxes/trevo-lang.tmLanguage.json"
        }]
    }
}